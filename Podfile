# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'MyDiary' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MyDiary

  # Logging
  pod 'SwiftyBeaver', '~> 1.9.1'

  # Resources
  pod 'R.swift', '~> 5.2.0'

  target 'MyDiaryTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick', '~> 3.0.0'
    pod 'Nimble', '~> 8.1.1'
  end

end
